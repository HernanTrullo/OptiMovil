<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:background="@color/white"
    tools:context=".fragments.BotonesFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@color/white"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/btn_search"
            android:layout_width="@dimen/ic_menu"
            android:layout_height="@dimen/ic_menu"
            android:layout_margin="10dp"
            android:background="@drawable/ic_btn_search_24"
            android:visibility="visible" />

        <ImageButton
            android:id="@+id/btn_home"
            android:layout_width="@dimen/ic_menu"
            android:layout_height="@dimen/ic_menu"
            android:layout_margin="10dp"
            android:background="@drawable/ic_btn_home_24" />
        <ImageButton
            android:id="@+id/btn_share"
            android:background="@drawable/ic_btn_share"
            android:layout_width="@dimen/ic_menu"
            android:layout_height="@dimen/ic_menu"
            android:layout_margin="10dp"/>
        <ImageButton
            android:id="@+id/btn_filter"
            android:background="@drawable/ic_btn_filtros_24"
            android:layout_width="@dimen/ic_menu"
            android:layout_height="@dimen/ic_menu"
            android:layout_margin="10dp"/>

        <ImageButton
            android:id="@+id/btn_likes"
            android:layout_width="@dimen/ic_menu"
            android:layout_height="@dimen/ic_menu"
            android:layout_margin="10dp"/>
        <ImageButton
            android:id="@+id/btn_settings"
            android:background="@drawable/ic_btn_settings_24"
            android:layout_width="@dimen/ic_menu"
            android:layout_height="@dimen/ic_menu"
            android:layout_margin="10dp"/>




    </LinearLayout>

</FrameLayout>